		<secure-properties:encrypt algorithm="Blowfish" />
	</secure-properties:config>
	<configuration doc:name="Configuration" doc:id="8382b23c-cd1d-4071-9d77-260d2025cab8" defaultErrorHandler-ref="error-handler" />
	<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="8ce8805b-ffa7-4ef9-ad64-0a8286df5b76" >
		<http:listener-connection host="${http.host}" port="${http.port}" />
	</http:listener-config>
</mule>
