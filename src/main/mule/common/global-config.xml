http://www.mulesoft.org/schema/mule/secure-properties http://www.mulesoft.org/schema/mule/secure-properties/current/mule-secure-properties.xsd
http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd">
	<configuration-properties doc:name="Configuration properties" doc:id="b06604a7-04ff-4dd5-b411-0805e336a2e0" file="properties\common.yaml" />
	<configuration-properties doc:name="Configuration properties" doc:id="bc7ae76e-27b7-4add-83c3-52804917f26b" file="properties\config-${env}.yaml" />
